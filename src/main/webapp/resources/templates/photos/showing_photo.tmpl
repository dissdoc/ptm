<li id="{id}">
    <div class="wrap">
        <a href="/photos/{id}?type={type}&type_id={type_id}">
            <img src="/data/{id}/showing.png"/>
        </a>
    </div>
    <div class="info">
        <p><a href="/photos/{id}?type={type}&type_id={type_id}">{name}</a></p>
        <p>{author}</p>
        <p>{fullDate}</p>
        <p><a href="/user/{userId}/photos">{user}</a></p>
    </div>
</li>